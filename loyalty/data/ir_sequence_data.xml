<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Sequences for sale.order -->
        <record id="seq_loyalty_deal" model="ir.sequence">
            <field name="name">Loyalty Deal</field>
            <field name="code">loyalty.deal</field>
            <field name="prefix">DEAL</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_loyalty_txn" model="ir.sequence">
            <field name="name">Transaction Request</field>
            <field name="code">loyalty.extra.txn.request</field>
            <field name="prefix">TXREQ</field>
            <field name="padding">3</field>
        </record>

         <record id="seq_loyalty_merchant_group" model="ir.sequence">
            <field name="name">Merchant Group</field>
            <field name="code">loyalty.merchant.group</field>
            <field name="prefix">GROUP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="product_grouping_service" model="product.product">
            <field name="name">Grouping Services</field>
        </record>
        <!-- 
        <record id="product_extra_txn_service" model="product.product">
            <field name="name">Extra Tnx Services</field>
        </record> -->

    </data>
</odoo>